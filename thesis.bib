}

@article{Barr:2006ih,
author = {Barr, G. D. and Robbins, S. and Gaisser, T. K. and Stanev, T.},
title = {{Uncertainties in atmospheric neutrino fluxes}},
journal = {Phys. Rev. D},
year = {2006},
volume = {74},
number = {9},
pages = {323},
month = Nov,
publisher = {American Physical Society},
doi = {10.1103/PhysRevD.74.094009},
language = {English},
rating = {0},
date-added = {2018-11-26T22:59:05GMT},
date-modified = {2018-11-26T22:59:21GMT},
abstract = {An evaluation of the principal uncertainties in the computation of neutrino fluxes produced in cosmic ray showers in the atmosphere is presented. The neutrino flux predictions are needed for comparison with experiment to perform neutrino oscillation studies. The paper concentrates on the main limitations which are due to hadron production uncertainties. It also treats primary cosmic ray flux uncertainties, which are at a lower level. The absolute neutrino fluxes are found to have errors of around 15\% in the neutrino energy region important for contained events underground. Large cancellations of these errors occur when ratios of fluxes are considered, in particular, the ${\ensuremath{\nu}}_{\ensuremath{\mu}}/{\overline{\ensuremath{\nu}}}_{\ensuremath{\mu}}$ ratio below ${E}_{\ensuremath{\nu}}=1\text{ }\text{ }\mathrm{GeV}$, the $({\ensuremath{\nu}}_{\ensuremath{\mu}}+{\overline{\ensuremath{\nu}}}_{\ensuremath{\mu}})/({\ensuremath{\nu}}_{e}+{\overline{\ensuremath{\nu}}}_{e})$ ratio below ${E}_{\ensuremath{\nu}}=10\text{ }\text{ }\mathrm{GeV}$ and the up/down ratios above ${E}_{\ensuremath{\nu}}=1\text{ }\text{ }\mathrm{GeV}$ are at the 1\% level. A detailed breakdown of the origin of these errors and cancellations is presented.},
url = {https://link.aps.org/doi/10.1103/PhysRevD.74.094009}
}

@article{Evans:2017hu,
author = {Evans, J. and others},
title = {{Uncertainties in atmospheric muon-neutrino fluxes arising from cosmic-ray primaries}},
journal = {Phys. Rev. D},
year = {2017},
volume = {95},
number = {2},
pages = {1180},
month = Jan,
publisher = {American Physical Society},
doi = {10.1103/PhysRevD.95.023012},
language = {English},
read = {Yes},
rating = {0},
date-added = {2018-11-26T21:04:30GMT},
date-modified = {2018-11-26T21:05:11GMT},
abstract = {We present an updated calculation of the uncertainties on the atmospheric muon-neutrino flux arising from cosmic-ray primaries. For the first time, we include recent measurements of the cosmic-ray primaries collected since 2005. We apply a statistical technique that allows the determination of correlations between the parameters of the Gaisser, Stanev, Honda, and Lipari primary-flux parametrization and the incorporation of these correlations into the uncertainty on the muon-neutrino flux. We obtain an uncertainty related to the primary cosmic rays of around (5--15)\%, depending on energy, which is about a factor of 2 smaller than the previously determined uncertainty. The hadron production uncertainty is added in quadrature to obtain the total uncertainty on the neutrino flux, which is reduced by $\ensuremath{\approx}5\%$. To take into account an unexpected hardening of the spectrum of primaries above energies of 100 GeV observed in recent measurements, we propose an alternative parametrization and discuss its impact on the neutrino flux uncertainties.},
url = {https://link.aps.org/doi/10.1103/PhysRevD.95.023012}
}


